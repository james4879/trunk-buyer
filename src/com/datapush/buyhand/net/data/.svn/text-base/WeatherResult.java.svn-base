package com.datapush.buyhand.net.data;


public class WeatherResult {
	private A weatherinfo;

	public A getWeatherinfo() {
		return weatherinfo;
	}

	public void setWeatherinfo(A weatherinfo) {
		this.weatherinfo = weatherinfo;
	}

	public class A {

		private String city;// 名字
		private String date_y;// 时间
		private String week;// 星期
		private String temp1;// "10℃~7℃"
		private String index_d;// 穿衣

		public String getCity() {
			return city;
		}

		public void setCity(String city) {
			this.city = city;
		}

		public String getDate_y() {
			return date_y;
		}

		public void setDate_y(String date_y) {
			this.date_y = date_y;
		}

		public String getWeek() {
			return week;
		}

		public void setWeek(String week) {
			this.week = week;
		}

		public String getTemp1() {
			return temp1;
		}

		public void setTemp1(String temp1) {
			this.temp1 = temp1;
		}

		public String getIndex_d() {
			return index_d;
		}

		public void setIndex_d(String index_d) {
			this.index_d = index_d;
		}
	}
}
